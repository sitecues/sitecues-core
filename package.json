{
  "name": "sitecues-core",
  "version": "0.0.3",
  "description": "Application launcher for Sitecues.",
  "homepage": "https://github.com/sitecues/sitecues-core",
  "main": "index.js",
  "author": {
    "name": "Seth Holladay",
    "url": "http://seth-holladay.com",
    "email": "me@seth-holladay.com"
  },
  "scripts": {
    "test": "xo",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sitecues/sitecues-core.git"
  },
  "bugs": {
    "url": "https://github.com/sitecues/sitecues-core/issues",
    "email": "me@seth-holladay.com"
  },
  "engines": {
    "node": ">=6"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "build-version": "^0.2.2",
    "delivr": "^0.4.0",
    "eslint-config-tidy": "^0.3.2",
    "gulp": "gulpjs/gulp#4.0",
    "pkg-dir": "^1.0.0",
    "read-pkg-up": "^1.0.1",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-replace": "^1.1.1",
    "xo": "^0.16.0"
  },
  "keywords": [
    "capabilities",
    "capability",
    "feature",
    "detection",
    "launcher",
    "core",
    "fallback",
    "metrics"
  ],
  "xo": {
    "extend": "tidy",
    "ignore": [
      "build/**",
      "latest-build/**"
    ],
    "overrides": [
      {
        "files": "lib/**",
        "env": "browser",
        "global": [
          "sitecues"
        ]
      }
    ]
  }
}
